<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <style name="ActionBarStyle" parent="@android:style/Widget.Holo.Light.ActionBar.Solid.Inverse">
        <item name="android:icon">@drawable/icon</item>
        <item name="android:background">#FF666666</item>
    </style>

    <style name="MyActionBar" parent="@style/Theme.AppCompat.Light">
        <item name="android:actionBarStyle">@style/MyactionBarStyle</item>
        <item name="actionBarStyle">@style/MyactionBarStyle</item>
    </style>

    <!-- <item name="android:background">@color/actionbarbg</item> -->
    <!-- <item name="background">@color/actionbarbg</item> -->
    <style name="MyactionBarStyle" parent="@style/Widget.AppCompat.ActionBar">
        <item name="android:background">@drawable/bottom_nav_background_drawable</item>
        <item name="background">@drawable/bottom_nav_background_drawable</item>
        <item name="android:titleTextStyle">@style/MyActionBarTitleText</item>
        <item name="titleTextStyle">@style/MyActionBarTitleText</item>
    </style>

    <style name="MyActionBarTitleText" parent="@style/TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textColor">@color/common_nav_text_color</item>
    </style>
    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="Home.Header" parent="@android:style/Theme" />

    <style name="Home.Header.Logo" parent="@style/Home.Header">
        <item name="android:scaleType">fitXY</item>
    </style>

    <style name="Home.Header.Search" parent="@style/Home.Header">
        <item name="android:paddingRight">5.0dip</item>
    </style>

    <style name="Home.Banner.Nav" parent="@style/Home.Header">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/banner_nav_text_color</item>
    </style>

    <style name="App.List" parent="@android:style/Theme" />

    <style name="Common.App.ListView.Item.Layout" parent="@style/App.List">
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/list_view_item_selector</item>
        <item name="android:paddingLeft">8.0dip</item>
    </style>

    <style name="Common.App.ListView.Item.Icon" parent="@style/App.List">
        <item name="android:background">@drawable/list_view_icon_bg</item>
        <item name="android:scaleType">centerInside</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="Common.App.ListView.Item.State.TV" parent="@style/App.List">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">@color/list_view_item_download_text_color</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:paddingTop">5.0dip</item>
    </style>

    <style name="Common.App.ListView.Item.Action" parent="@style/App.List">
        <item name="android:textSize">10.0sp</item>
        <item name="android:textColor">@color/list_view_item_size_color</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">10.0dip</item>
        <item name="android:maxLines">1</item>
    </style>

    <style name="Common.App.ListView.Item.Title" parent="@style/App.List">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/list_view_item_title_color</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:maxLines">1</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="Common.App.ListView.Item.Size" parent="@style/App.List">
        <item name="android:textSize">10.0sp</item>
        <item name="android:textColor">@color/list_view_item_size_color</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:maxLines">1</item>
    </style>

    <style name="Common.App.ListView.Item.SubTitle" parent="@style/App.List">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">@color/list_view_item_subtitle_color</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:maxLines">1</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="Manage.App.ListView.Item.Title" parent="@style/App.List">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:maxLines">1</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="TextView.Font12AndGray" parent="@style/TextView">
        <item name="android:textColor">#ff565656</item>
    </style>

    <style name="TextView">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">#ff000000</item>
    </style>

    <style name="ProgressBar_download_bar">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/progressbar_item_download</item>
    </style>

    <style name="TextView.Font12AndPressColor" parent="@style/TextView">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">@drawable/btn_pressed_txt_color</item>
    </style>

    <style name="TextView.Font10AndPressColor" parent="@style/TextView">
        <item name="android:textSize">10.0sp</item>
        <item name="android:textColor">@drawable/btn_pressed_txt_color</item>
    </style>

    <style name="TextView.Font16AndBlack" parent="@style/TextView">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="TextView.Font16AndPressColor" parent="@style/TextView">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@drawable/btn_pressed_txt_color</item>
    </style>

    <style name="TextView.Font14AndGray" parent="@style/TextView">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#ff9a9a9a</item>
    </style>

    <style name="TextView.Font14AndWhite" parent="@style/TextView">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="AP_progressBar_download">
        <item name="android:maxHeight">8.0dip</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/ap_progress_bar_download</item>
        <item name="android:minHeight">8.0dip</item>
    </style>

    <style name="Channel.Header.Title" parent="@style/Channel.Header">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/top_nav_title_color</item>
    </style>

    <style name="Channel.Header" parent="@android:style/Theme" />

    <style name="TextView.Font16AndGray" parent="@style/TextView">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">#ff9a9a9a</item>
    </style>

    <style name="AP_ResourcesLoadingProgress" parent="@android:style/Widget.ProgressBar">
        <item name="android:maxWidth">20.0dip</item>
        <item name="android:maxHeight">20.0dip</item>
        <item name="android:indeterminateDrawable">@drawable/ap_resource_progress_medium</item>
        <item name="android:minWidth">20.0dip</item>
        <item name="android:minHeight">20.0dip</item>
    </style>

    <style name="ResourcesLoadingProgress" parent="@android:style/Widget.ProgressBar">
        <item name="android:maxWidth">20.0dip</item>
        <item name="android:maxHeight">20.0dip</item>
        <item name="android:indeterminateDrawable">@drawable/loading_row</item>
        <item name="android:minWidth">20.0dip</item>
        <item name="android:minHeight">20.0dip</item>
    </style>

    <style name="TextView.Font14AndBlack" parent="@style/TextView">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#ff000000</item>
    </style>

    <style name="Resource.Detail.Desc" parent="@style/App.List">
        <item name="android:textSize">12.0dip</item>
        <item name="android:textColor">@color/list_view_item_size_color</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:lineSpacingExtra">4.0dip</item>
    </style>

    <style name="appRatingBarSmall" parent="@android:style/Widget.RatingBar">
        <item name="android:maxHeight">11.0dip</item>
        <item name="android:progressDrawable">@drawable/app_ratingbar_small_layer</item>
        <item name="android:minHeight">11.0dip</item>
    </style>

    <style name="Common.App.ListView" parent="@style/App.List">
        <item name="android:scrollbarStyle">outsideOverlay</item>
        <item name="android:fadingEdge">none</item>
        <item name="android:descendantFocusability">blocksDescendants</item>
        <item name="android:cacheColorHint">@color/white</item>
        <item name="android:divider">@drawable/list_view_divider</item>
        <item name="android:dividerHeight">0.0dip</item>
        <item name="android:footerDividersEnabled">false</item>
    </style>

    <style name="Category.App.ListView.Item.Banner" parent="@style/App.List">
        <item name="android:scaleType">centerInside</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="Category.App.ListView.Item.Layout" parent="@style/App.List">
        <item name="android:background">@drawable/list_view_category_item_selector</item>
        <item name="android:descendantFocusability">afterDescendants</item>
    </style>

    <style name="Common.App.ListView.Item.RightArrow.IV" parent="@style/App.List">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:descendantFocusability">beforeDescendants</item>
    </style>

    <style name="MyCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/check_box_selector</item>
        <item name="android:paddingLeft">20.0dip</item>
        <item name="android:maxHeight">15.0dip</item>
    </style>

    <!-- <item name="android:button">@drawable/check_box_selector</item> -->
    <style name="MyRadioButton" parent="@android:style/Widget.CompoundButton.RadioButton">
        <item name="android:button">@null</item>
        <item name="android:drawableLeft">@drawable/check_box_selector</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">5.0dip</item>
        <item name="android:drawablePadding">20.0dip</item>
    </style>

    <style name="ProgressBar_download">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/progressbar_download</item>
    </style>

    <style name="resourceReportItemStyle">
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">5.0dip</item>
        <item name="android:drawableLeft">@drawable/check_box_selector</item>
        <item name="android:drawablePadding">10.0dip</item>
    </style>

    <style name="TextView.Font14AndPressColor" parent="@style/TextView">
        <item name="android:textSize">14.0dip</item>
        <item name="android:textColor">@drawable/btn_pressed_txt_color</item>
    </style>

    <style name="roomRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/ratingbar_drawable</item>
    </style>

